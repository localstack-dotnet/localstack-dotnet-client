{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "SampleS3Service": "Debug"
    }
  },
  "LocalStack": {
    "UseLocalStack": true,
    "Session": {
      "AwsAccessKeyId": "my-AwsAccessKeyId",
      "AwsAccessKey": "my-AwsAccessKey",
      "AwsSessionToken": "my-AwsSessionToken",
      "RegionName": "eu-central-1"
    },
    "Config": {
      "LocalStackHost": "localhost.localstack.cloud",
      "UseSsl": false,
      "UseLegacyPorts": false,
      "EdgePort": 4566
    }
  }
}